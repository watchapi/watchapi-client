{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",

      "runtimeExecutable": "${execPath}",

      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],

      "preLaunchTask": "watch",

      "outFiles": ["${workspaceFolder}/dist/**/*.js"],

      "sourceMaps": true
    }
  ]
}
